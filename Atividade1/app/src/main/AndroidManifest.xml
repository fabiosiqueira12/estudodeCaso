<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fabio.atividade1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AcompanhamentoActivity"  android:label="@string/activity_acompanhamento"/>
        <activity android:name=".NoticiaActivity" android:label="@string/activity_noticia"/>
        <activity android:name=".ContatoActivity" android:label="@string/activity_contato"></activity>
    </application>

</manifest>